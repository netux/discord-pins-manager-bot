{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"compilerOptions": {
		"module": "Node16",
		"moduleResolution": "Node16",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strictNullChecks": true,
		"outDir": "dist",
		"rootDir": ".",
		"declaration": false,
		"declarationMap": false,
		"sourceMap": true,
		"isolatedModules": true
	},
	"exclude": [
		"node_modules/**/*"
	]
}
