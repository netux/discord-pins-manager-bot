	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch bot",
			"type": "node",
			"request": "launch",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run",
				"dev"
			],
			"sourceMaps": true,
			"skipFiles": [
				"<node_internals>/**"
			],
			"outFiles": [
				"${workspaceFolder}/dist/**/*.js"
			]
		}
	]
}
